{
    "name": "genial-framework/web-application-manager",
    "type": "library",
    "keywords": [
        "php72",
        "php-library",
        "genial-framework",
        "web-application-manager",
        "fann",
        "json",
        "yaml",
        "automated",
        "secure-coding",
        "performance-manager",
        "artificial-neural-network",
        "php"
    ],
    "description": "A fast artificial intelligence library to automatically manage your websites.",
    "homepage": "https://github.com/genial-framework/web-application-manager",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Repositroy Contributors",
            "homepage": "https://github.com/genial-framework/web-application-manager/graphs/contributors",
            "role": "Contribute"
        }
    ],
    "require": {
        "php": "^7.2",
        "pimple/pimple": "^3",
        "symfony/yaml": "^4",
        "psr/container": "*",
        "ext-fann": "*",
        "ext-json": "*",
        "ext-yaml": "*"
    },
    "suggest": {
        "ext-xml": "To log data in xml format.",
        "ext-pdo": "To log data in a database."
    },
    "autoload": {
        "psr-4": {
            "Genial\\App\\": "./src"
        },
        "files": ["./setup.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
